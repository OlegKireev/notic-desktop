{
  "name": "notic-desktop",
  "version": "0.1.0",
  "description": "An Electron client for a notes managing app",
  "main": "src/main.js",
  "scripts": {
    "start": "electron src/main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "app.netlify.notic",
    "productName": "Notic",
    "directories": {
      "buildResources": "resources"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlegKireev/notic-desktop.git"
  },
  "author": "Oleg Kireev <flucky@mail.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OlegKireev/notic-desktop/issues"
  },
  "homepage": "https://notic.netlify.app/",
  "devDependencies": {
    "electron": "17.0.0",
    "electron-builder": "22.14.5",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4"
  },
  "dependencies": {
    "electron-util": "0.17.2"
  }
}